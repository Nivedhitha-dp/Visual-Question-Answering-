        "        inputs = {key: value.to(device) for key, value in inputs.items()}\n",
        "\n",
        "        # Forward pass through the model using generate\n",
        "        outputs = model.generate(input_ids=inputs['input_ids'], pixel_values=inputs['pixel_values'], max_length=32)\n",
        "\n",
        "        # Decode predicted answers\n",
        "        predicted_answers = processor.tokenizer.batch_decode(outputs, skip_special_tokens=True)\n",
        "        # Compare predictions with ground-truth answers\n",
        "        for pred, true in zip(predicted_answers, answers):\n",
        "            if pred.strip().lower() == true.strip().lower():\n",
        "                correct += 1\n",
        "            total += 1\n",
